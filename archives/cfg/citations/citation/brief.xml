<?xml version="1.0" ?>

<!-- 
	Brief citation style for, ahem, citation data.
-->

<cite:citation xmlns="http://www.w3.org/1999/xhtml" xmlns:cite="http://eprints.org/ep3/citation" xmlns:epc="http://eprints.org/ep3/control">
  <span>
    <epc:print expr="source" /> <epc:print expr="cluster" /> (<epc:print expr="datestamp"/>):
    <epc:phrase ref="citation_fieldname_impact" /> <epc:print expr="impact" />
  </span>
</cite:citation>
